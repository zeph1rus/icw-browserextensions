{
    "manifest_version": 3,
    "name": "ICW Push",
    "description": "Base Level Extension",
    "version": "1.1",
    "background": { 
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "preferences.html",
      "default_icon": "icons/icw_logo_48.png"
    },
    "permissions": [
      "contextMenus", "notifications", "storage"
    ],
    "icons": {
      "16": "icons/icw_logo_16.png",
      "48": "icons/icw_logo_48.png"
    },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}