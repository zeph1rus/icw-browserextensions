{
    "manifest_version": 3,
    "name": "ICW Push",
    "description": "ICW Local and Remote Push",
    "version": "1.5",
    "background": { 
      "scripts": ["background.js"]    
    },
    "action": {
      "default_popup": "preferences.html",
      "default_icon": "icons/icw_logo_48.png"
    },
    "permissions": [
      "contextMenus", "notifications", "storage"
    ],
    "icons": {
      "16": "icons/icw_logo_16.png",
      "48": "icons/icw_logo_48.png"
    },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "icw@9600bps.net"
    }
  }
}